generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  login     String
  password  String
  version   Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
